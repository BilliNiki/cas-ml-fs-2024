[('columntransformer', ColumnTransformer(transformers=[('numerical', StandardScaler(),
                                 ['cap-diameter', 'stem-height', 'stem-width',
                                  'season']),
                                ('categorical',
                                 OneHotEncoder(sparse_output=False),
                                 ['cap-shape', 'gill-attachment', 'gill-color',
                                  'stem-color'])])), ('kneighborsclassifier', KNeighborsClassifier(n_neighbors=3))]